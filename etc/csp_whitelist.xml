<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="connect-src">
            <values>
                <value id="shapediver" type="host">https://*.shapediver.com</value>
            </values>
        </policy>

        <policy id="font-src">
            <values>
                <value id="fontawesome" type="host">https://*.fontawesome.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="fontawesome" type="host">https://*.fontawesome.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="data-uri" type="host">data:</value>
                <value id="configbox-website" type="host">https://*.configbox.at</value>
                <value id="shapediver" type="host">https://*.shapediver.com</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="configbox-website" type="host">https://*.configbox.at</value>
                <value id="shapediver" type="host">https://*.shapediver.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
